{
  "name": "kidsactivitytracker-backend",
  "version": "1.0.0",
  "description": "Backend API for Kids Activity Tracker",
  "main": "server.js",
  "scripts": {
    "start": "node api/server.js",
    "dev": "nodemon api/server.js",
    "monitoring": "node monitoring/dashboard.js",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:seed": "node prisma/seed.js",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    "docker:build": "docker build -t kids-activity-api .",
    "docker:run": "docker run -p 3000:3000 --env-file .env kids-activity-api",
    "gcp:deploy": "gcloud builds submit --config=cloudbuild.yaml ."
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "dependencies": {
    "@prisma/client": "^6.12.0",
    "axios": "^1.10.0",
    "bull": "^4.16.5",
    "cheerio": "^1.1.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.6.1",
    "morgan": "^1.10.0",
    "node-cron": "^4.2.1",
    "pg": "^8.16.3",
    "prisma": "^6.12.0",
    "puppeteer": "^22.8.2",
    "puppeteer-core": "^24.14.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs"
}
