{
  "name": "kidsactivitytracker-backend",
  "version": "1.0.0",
  "description": "Backend API for Kids Activity Tracker",
  "main": "server.js",
  "scripts": {
    "start": "node dist/server.js",
    "start:legacy": "node api/server.js",
    "dev": "nodemon --watch src --ext ts --exec ts-node src/server.ts",
    "dev:legacy": "nodemon api/server.js",
    "build": "tsc || true",
    "watch": "tsc -w",
    "monitoring": "node monitoring/dashboard.js",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:seed": "node prisma/seed.js",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    "docker:build": "docker build -t kids-activity-api .",
    "docker:run": "docker run -p 3000:3000 --env-file .env kids-activity-api",
    "gcp:deploy": "gcloud builds submit --config=cloudbuild.yaml .",
    "test": "jest --config ../__tests__/backend/jest.config.js",
    "test:unit": "jest --config ../__tests__/backend/jest.config.js --testPathPattern='unit'",
    "test:routes": "jest --config ../__tests__/backend/jest.config.js --testPathPattern='routes'",
    "test:integration": "jest --config ../__tests__/backend/jest.config.js --testPathPattern='integration'",
    "test:coverage": "jest --config ../__tests__/backend/jest.config.js --coverage",
    "test:watch": "jest --config ../__tests__/backend/jest.config.js --watch"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@langchain/core": "^1.1.8",
    "@langchain/langgraph": "^1.0.7",
    "@langchain/openai": "^1.2.0",
    "@prisma/client": "^6.12.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "bull": "^4.16.5",
    "cheerio": "^1.1.0",
    "cors": "^2.8.5",
    "csv-parse": "^5.6.0",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.2.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.5",
    "pg": "^8.16.3",
    "prisma": "^6.12.0",
    "puppeteer": "^22.8.2",
    "puppeteer-core": "^24.14.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0",
    "xlsx": "^0.18.5",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^1.4.13",
    "@types/node": "^24.2.0",
    "@types/nodemailer": "^6.4.17",
    "@types/supertest": "^2.0.0",
    "@types/uuid": "^10.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs"
}
